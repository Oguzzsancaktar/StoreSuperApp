{
  "expo": {
    "name": "Setuka24",
    "slug": "Setuka24",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "setuka24",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FEF9B0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ngxmarketplace.setuka24",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FEF9B0"
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to your photos to upload and display images."
      },
      "config": {
        "googleSignIn": {
          "reservedClientId": "877906952522-4t2qllnb7doo0vg09fat26ha13t8lfsq.apps.googleusercontent.com"
        }
      },
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ngxmarketplace.setuka24"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "776cacd9-0a7b-4ad3-87a8-0f94d20888b8"
      }
    }
  }
}
